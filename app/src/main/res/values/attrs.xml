<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MaterialRadioButton">
        <attr name="checked" format="boolean" />
        <attr name="text" format="string" />
        <attr name="textSize" format="integer" />
        <attr name="direction" format="integer" />
        <attr name="mode" format="integer" />
        <attr name="textColor" format="integer" />
    </declare-styleable>

    <declare-styleable name="AppIcon">
        <attr name="textf" format="string" />
        <attr name="image" format="integer" />
    </declare-styleable>

    <declare-styleable name="RippleView">
        <attr name="rv_alpha" format="integer" />
        <attr name="rv_framerate" format="integer"/>
        <attr name="rv_rippleDuration" format="integer"/>
        <attr name="rv_zoomDuration" format="integer" />
        <attr name="rv_color" format="color" />
        <attr name="rv_centered" format="boolean" />
        <attr name="rv_type" format="enum">
            <enum name="simpleRipple" value="0"/>
            <enum name="doubleRipple" value="1"/>
            <enum name="rectangle" value="2" />
        </attr>
        <attr name="rv_ripplePadding" format="dimension" />
        <attr name="rv_zoom" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MaterialTwoLineText">
    <attr name="secondaryTextColor" format="integer" />
        <attr name="primaryTextColor" format="integer" />
        <attr name="secondaryTextSize" format="integer" />
        <attr name="primaryTextSize" format="integer" />
        <attr name="secondaryTextMaxLines" format="integer" />
        <attr name="secondaryText" format="string" />
        <attr name="primaryText" format="string" />
    </declare-styleable>
</resources>